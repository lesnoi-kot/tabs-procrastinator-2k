{
  "$schema": "https://json.schemastore.org/webextension.json",
  "manifest_version": 2,
  "name": "Tabs heap",
  "version": "1.0",
  "description": "Display tabs count, close old tabs.",
  "icons": {
    "48": "icons/logo.png",
    "96": "icons/logo_x2.png"
  },
  "browser_action": {
    "default_icon": "icons/logo.png",
    "default_title": "Tabs info",
    "default_popup": "popup/index.html",
    "browser_style": true
  },
  "permissions": ["tabs"]
}
